f <- function(x9){
  y9 <- 1
  g <- function(y9){
    (x9+y9)/2
  }
  g(x9)
}
f(sqrt(5))

f <- function(x8){
  y8 <-1
  g(x8)
}
g <- function(y8){
  (x8+y8)/2
}
f(sqrt(5))

hypotenuse<- function(x,y){
  z=sqrt(x^2+y^2)
  return(z)
}
hypotenuse(4,2)
pig <- function(x=hypotenuse,y=12){
  z=sqrt(x^2+y^2)
  print(z)
}
pig(1)
